{
  "enabled": true,
  "extends": [
    "github>CatchoftheDay/renovate-config"
  ],
  "regexManagers": [
    {
      "description": "Update Buildkit version in pipeline.py",
      "fileMatch": [".*pipeline.py$"],
      "matchStrings": [
        "# renovate: datasource=(?<datasource>[a-z-]+?) depName=(?<depName>.+?)\\n.+?'(?<currentValue>v\\d+\\.\\d+\\.\\d+)'\\)"
      ]
    }
  ]
}
